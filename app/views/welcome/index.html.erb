<div class="top-bg"></div>
<h1>Welcome to DarkChamber!</h1>


<div>
    <% if current_user %>
        <%= render "header" %>
        <div class="post">
            <% current_user.feed.each do |feed_item| %>
                <div class="header">
                  <%= feed_item.user.full_name %>
                </div>
                <div class="image">
                  <%= image_tag feed_item.images.standard_resolution.url %>
                </div>
                <div>
                </div>
            <% end %>
            <br>
        </div>
    <% else %>
        <%= link_to "Login with Instagram", login_instagram_path %>
    <% end %>
</div>


